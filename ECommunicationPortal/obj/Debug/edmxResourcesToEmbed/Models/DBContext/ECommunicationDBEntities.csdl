  <Association Name="FK_Folder_Mailbox">
    <End Role="Mailbox" Type="Self.Mailbox" Multiplicity="0..1" />
    <End Role="Folder" Type="Self.Folder" Multiplicity="*" />
    <ReferentialConstraint>
      <Principal Role="Mailbox">
        <PropertyRef Name="MailboxId" />
      </Principal>
      <Dependent Role="Folder">
        <PropertyRef Name="MailboxId" />
      </Dependent>
    </ReferentialConstraint>
  </Association>
  <Association Name="FK_Folder_UserMailbox">
    <End Role="UserMailbox" Type="Self.UserMailbox" Multiplicity="0..1" />
    <End Role="Folder" Type="Self.Folder" Multiplicity="*" />
    <ReferentialConstraint>
      <Principal Role="UserMailbox">
        <PropertyRef Name="UserMailboxId" />
      </Principal>
      <Dependent Role="Folder">
        <PropertyRef Name="UserMailboxId" />
      </Dependent>
    </ReferentialConstraint>
  </Association>
  <Association Name="FK_UserMailbox_Users">
    <End Role="Users" Type="Self.User" Multiplicity="1" />
    <End Role="UserMailbox" Type="Self.UserMailbox" Multiplicity="*" />
    <ReferentialConstraint>
      <Principal Role="Users">
        <PropertyRef Name="UserId" />
      </Principal>
      <Dependent Role="UserMailbox">
        <PropertyRef Name="UserId" />
      </Dependent>
    </ReferentialConstraint>
  </Association>
  <EntityContainer Name="ECommunicationEntities" annotation:LazyLoadingEnabled="true">
    <EntitySet Name="Files" EntityType="Self.File" />
    <EntitySet Name="Folders" EntityType="Self.Folder" />
