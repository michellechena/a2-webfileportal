    <AssociationSet Name="FK_UserMailbox_Users" Association="Self.FK_UserMailbox_Users">
      <End Role="Users" EntitySet="Users" />
      <End Role="UserMailbox" EntitySet="UserMailboxes" />
    </AssociationSet>
  </EntityContainer>
</Schema>